<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.noobstangofdusk.cb.jfxtu1337.ac33FXML.sample.Controller">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0">
          
          <Label text="Difficulty" />
          
          <RadioButton text="Easy">
            <toggleGroup>
               <ToggleGroup fx:id="toggleGroup" />
            </toggleGroup>
         </RadioButton>
          <RadioButton text="Medium" toggleGroup="$toggleGroup" />
          <RadioButton text="Hard" toggleGroup="$toggleGroup" />
          
      </VBox>
   </children>
</AnchorPane>
